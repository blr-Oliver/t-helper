{
    "extends": "../tslint.json",
    "rules": {
      "curly": false,
      "interface-over-type-literal": false,
      "no-bitwise": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
